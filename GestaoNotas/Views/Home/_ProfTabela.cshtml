@model List<GestaoNotas.Models.ViewModel.ProfViewModel>

<div class="container my-3">
    <div class="my-3">
        Cadastradas
    </div>
    <div class="row">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Nome</th>
                    <th scope="col">Campus</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Count(); i++)
                {
                    <tr>
                        <th scope="row">
                            @Html.DisplayFor(modelItem => modelItem[i].IdProfessor);
                        </th>
                        <td>
                            @Html.DisplayFor(modelItem => modelItem[i].Nome);
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => modelItem[i].Campus);
                        </td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>